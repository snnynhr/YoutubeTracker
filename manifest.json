{
  "manifest_version": 2,

  "name": "YouTubeTracker",
  "description": "YT Tracker.",
  "version": "1.0",

  "permissions": [
  	"tabs",
	"activeTab",
	"unlimitedStorage",
	"http://*/*",
	"https://*/*"
  ],
  "content_scripts" : [
  {
  	"matches": ["*://www.youtube.com/*"],
  	"js": ["util/jquery.js"],
  	"run_at": "document_start"
  }
  ],
   "background" : {
		"page": "background.html",
		"persistent": true
	}
}